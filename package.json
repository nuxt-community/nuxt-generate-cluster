{
  "name": "nuxt-generate-cluster",
  "version": "1.0.0-rc11",
  "description": "multi-threaded generate for nuxt using cluster",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && cross-env NODE_ENV=test npm run build:generator && nyc ava --verbose --serial test/ -- && nyc report --reporter=html",
    "just-test": "nyc ava --verbose --serial test/",
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "lint": "eslint --ext .js,.vue bin lib pages test/*.js --ignore-pattern app",
    "build": "rimraf dist/ && npm run build:generator",
    "build:generator": "rollup -c build/rollup.config.js --environment TARGET:generator",
    "release-next": "npm run build && node ./build/release-next && npm publish --tag next"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/nuxt-generate-cluster.git"
  },
  "files": [
    "bin",
    "dist",
    "lib",
    "index.js"
  ],
  "keywords": [
    "nuxt",
    "generate",
    "multithread",
    "cluster"
  ],
  "bin": {
    "nuxt-generate": "./bin/nuxt-generate"
  },
  "nyc": {
    "include": [
      "lib"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxt-community/nuxt-generate-cluster/issues"
  },
  "homepage": "https://github.com/nuxt-community/nuxt-generate-cluster#readme",
  "engines": {
    "node": ">=6.11"
  },
  "dependencies": {
    "data-store": "^1.0.0",
    "nuxt": "^1.0.0-gh-adc7260"
  },
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-eslint": "^8.0.1",
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "codecov": "^3.0.0",
    "copy-webpack-plugin": "^4.1.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.8.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.2.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.1",
    "finalhandler": "^1.1.0",
    "jsdom": "^11.3.0",
    "json-loader": "^0.5.7",
    "nyc": "^11.2.1",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.52.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-watch": "^4.3.1",
    "sinon": "^4.0.2",
    "std-mocks": "^1.0.1",
    "uglify-js": "^3.1.3"
  }
}
