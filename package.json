{
  "name": "nuxt-generate-cluster",
  "version": "1.0.0-rc11",
  "description": "multi-threaded generator for nuxt using cluster",
  "main": "index.js",
  "scripts": {
    "build": "rimraf dist/ && npm run build:generator",
    "build:generator": "rollup -c build/rollup.config.js --environment TARGET:generator"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nuxt-community/nuxt-generate-cluster.git"
  },
  "keywords": [
    "nuxt",
    "generate",
    "multithread",
    "cluster"
  ],
  "bin": {
    "nuxt-generate": "./bin/nuxt-generate"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nuxt-community/nuxt-generate-cluster/issues"
  },
  "homepage": "https://github.com/nuxt-community/nuxt-generate-cluster#readme",
  "dependencies": {
    "data-store": "^1.0.0",
    "nuxt": "^1.0.0-rc11"
  },
  "devDependencies": {
    "babel-plugin-array-includes": "^2.0.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-istanbul": "^4.1.5",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "fs-extra": "^4.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^0.50.0",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-babel": "^3.0.2",
    "rollup-plugin-commonjs": "^8.2.4",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^2.0.0"
  }
}
